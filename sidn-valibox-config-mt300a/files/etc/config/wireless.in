config wifi-device 'radio1'
        option type 'mac80211'
        option path 'platform/ar933x_wmac'
        option txpower '30'
        option country 'US'
        option hwmode '11a'
        option channel 'auto'

config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/10180000.wmac'
        option htmode 'HT20'
        option country '00'

config wifi-iface
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option encryption 'none'
        option ssid 'SIDN-ValiBox-XHWADDRX'

config wifi-iface
        option device 'radio1'
        option mode 'ap'
        option encryption 'none'
        option ssid 'NopenWrt'
